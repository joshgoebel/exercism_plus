{
    "name": "Exercism Plus",
    "version": "0.6",
    "description": "Mentor enhancements for the great Exercism website!",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://exercism.io/*"],
          "css": ["styles.css"],
          "js": ["runtime.js"],
          "run_at": "document_start"
        }
      ],
    "icons": {
        "128": "images/exercism_128.png",
        "48": "images/exercism_48.png",
        "32": "images/exercism_32.png",
        "16": "images/exercism_16.png"
    }
  }